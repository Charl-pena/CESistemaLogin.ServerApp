@inherits LayoutComponentBase

<AuthorizeView Roles="UserNoMFA">
   <Authorized>
      <MFALogicComponent />
   </Authorized>
</AuthorizeView>

<AuthorizeView Roles="User, Administrator">
   <Authorized>
      <Aplicacion>
            @Body
      </Aplicacion>
   </Authorized>
</AuthorizeView>

<AuthorizeView Roles="Administrator, UserNoMFA, User">
